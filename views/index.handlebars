
<div>
	<h1>Lista de Productos</h1>
	{{#if prodc}}
		{{#each prodc}}
			<h2>{{this.title}}</h2>
			<p>{{this.description}}</p>
			<p>{{this.code}}</p>
			<p>{{this.price}}</p>
			<p>{{this.stock}}</p>
			<p>{{this.category}}</p>
			{{#if this.thumbnails}}
			{{/if}}
		{{/each}}
	{{else}}
		<h3>No hay productos agregados recientemente</h3>
	{{/if}}
</div>